@model IEnumerable<LeftAside>

<aside class="left">
    @foreach (LeftAside leftAside in Model)
    {
        <div>
            <a href="@leftAside.Link" class="@leftAside.ColorClass" target="_blank">@leftAside.Title</a>
            <a href="@leftAside.Link" class="@leftAside.ColorClass" target="_blank"> <i class="@leftAside.IconClass"></i> </a>
        </div>
    }
</aside>